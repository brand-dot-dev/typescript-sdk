{
  "dxt_version": "0.2",
  "name": "brand.dev-mcp",
  "version": "0.7.0",
  "description": "The official MCP Server for the Brand Dev API",
  "author": {
    "name": "Brand Dev",
    "email": "hello@brand.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brand-dot-dev/typescript-sdk.git"
  },
  "homepage": "https://github.com/brand-dot-dev/typescript-sdk/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.brand.dev/",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "BRAND_DEV_API_KEY": "${user_config.BRAND_DEV_API_KEY}"
      }
    }
  },
  "user_config": {
    "BRAND_DEV_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
